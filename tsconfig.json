{
	"compilerOptions": {
		"baseUrl": ".",
		"outDir": "dist",
		"esModuleInterop": true,
		"target": "ES2020",
		"module": "CommonJS",
		"strict": true,
		"noUnusedParameters": true,
		"moduleResolution": "node",
		"alwaysStrict": true,
		"removeComments": true
	},
	"exclude": ["node_modules", "dist"]
}
